from errorAbsoluto import ErrorAbsoluto

class ErrorRelativo(ErrorAbsoluto):
    def __init__(self, realValue, stimatedValue):
        super().__init__(realValue, stimatedValue)

    def calcular(self,stimatedValue):
        errorRelative = (super().calcular() / self.stimatedValue) * 100
        return errorRelative
    
x = ErrorRelativo(15, 14.5)
print(x.calcular(14.5))  # Output: 0.5